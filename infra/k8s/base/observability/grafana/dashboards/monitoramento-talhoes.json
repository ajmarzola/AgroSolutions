          "editorMode": "code",
          "format": "table",
          "rawSql": "SELECT \r\n  t.IdTalhao,\r\n  t.UmidadeSoloPercentual as UltimaUmidade,\r\n  CASE \r\n    WHEN t.UmidadeSoloPercentual < 30 THEN 'Risco' \r\n    ELSE 'OK' \r\n  END as Status\r\nFROM (\r\n  SELECT \r\n    IdTalhao, \r\n    UmidadeSoloPercentual, \r\n    ROW_NUMBER() OVER (PARTITION BY IdTalhao ORDER BY DataHoraCapturaUtc DESC) as rn \r\n  FROM Leitura\r\n) t \r\nWHERE t.rn = 1",
