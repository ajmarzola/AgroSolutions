{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DefaultConnection": "Server=tcp:agrosolutions.database.windows.net,1433;Initial Catalog=Usuarios;Persist Security Info=False;User ID=usr_agro;Password=${DB_PASSWORD};MultipleActiveResultSets=False;Encrypt=True;TrustServerCertificate=False;Connection Timeout=30;"
  },
  "Jwt": {
    "Key": "${JWT_SECRET}",
    "Issuer": "AgroSolutions.Identity",
    "Audience": "AgroSolutions.Apps"
  },
  "OpenTelemetry": {
    "Endpoint": "http://localhost:4317",
    "Enabled": true
  }
}