{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DefaultConnection": "Server=db;Database=AgroSolutionsUsuarios;User Id=sa;Password={DB_PASSWORD};"
  },
  "Jwt": {
    "Key": "${JWT_SECRET}",
    "Issuer": "AgroSolutions.Identity",
    "Audience": "AgroSolutions.Apps"
  },
  "OpenTelemetry": {
    "Endpoint": "http://localhost:4317",
    "Enabled": true
  }
}