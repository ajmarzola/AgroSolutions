{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "SqlServer": {
    "ConnectionString": "Server=db;Database=AgroSolutionsIngestao;User Id=sa;Password={DB_PASSWORD};"
  },
  "OpenTelemetry": {
    "Endpoint": "http://otel-collector:4317",
    "Enabled": true
  },
  "RabbitMq": {
    "HostName": "rabbitmq",
    "Port": 5672,
    "UserName": "TOKEN_KEY_HERE",
    "Password": "TOKEN_KEY_HERE",
    "Enabled": true,
    "Exchange": "agrosolutions_eventos",
    "RoutingKeyLeituraRecebida": "leitura.recebida"
  },
  "Jwt": {
     "Key": "",
     "Issuer": "AgroSolutions.Identity",
     "Audience": "AgroSolutions.Apps"
  }
}